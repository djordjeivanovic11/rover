# Convert between coordinate systems
string conversion_type     # "gps_to_map", "map_to_gps", "gps_to_utm", "utm_to_gps"

# GPS coordinates (input/output)
float64 latitude
float64 longitude  
float64 altitude

# Map coordinates (input/output)
float64 map_x
float64 map_y
float64 map_z

# UTM coordinates (output)
float64 utm_easting
float64 utm_northing
string utm_zone
