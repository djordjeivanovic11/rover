<?xml version="1.0"?>
<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="SampleMission">
      <Action ID="LogMessage" message="Starting sample mission"/>
      <Action ID="NavigateToWaypoint" goal="1.0;2.0;0.0"/>
      <Action ID="LogMessage" message="Reached waypoint"/>
      <Action ID="CollectSample"/>
      <Action ID="LogMessage" message="Sample collected"/>
      <Action ID="NavigateToWaypoint" goal="0.0;0.0;0.0"/>
      <Action ID="LogMessage" message="Mission completed"/>
    </Sequence>
  </BehaviorTree>
</root>
