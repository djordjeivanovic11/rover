<?xml version="1.0"?>
<package format="3">
  <name>rover_boot</name>
  <version>0.1.0</version>
  <description>Top-level bring-up and supervisor for the URC rover.</description>

  <maintainer email="dorde_ivanovic@college.harvard.edu">Dorde Ivanovic</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_python</buildtool_depend>

  <exec_depend>launch</exec_depend>
  <exec_depend>launch_ros</exec_depend>

  <exec_depend>rclpy</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>
  <exec_depend>nav_msgs</exec_depend>

  <exec_depend>zed2i_launch</exec_depend>
  <exec_depend>slam_launch</exec_depend>
  <exec_depend>loc_fusion</exec_depend>
  <exec_depend>nav2_launch</exec_depend>
  <exec_depend>gap_guidance</exec_depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
